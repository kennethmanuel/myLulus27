<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nmp.mylulus27">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyLulus27"
        android:usesCleartextTraffic="true">
        <activity android:name="com.nmp.mylulus27.UbahMatkulActivity"></activity>
        <activity android:name="com.nmp.mylulus27.Register"></activity>
        <activity android:name="com.nmp.mylulus27.CardMatkul" />
        <activity android:name="com.nmp.mylulus27.MainActivity"></activity>
        <activity android:name="com.nmp.mylulus27.AddMatKulActivity"></activity>
        <activity android:name="com.nmp.mylulus27.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>